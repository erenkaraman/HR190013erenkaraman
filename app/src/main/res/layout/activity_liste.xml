<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.ListeActivity">




        <LinearLayout
            android:orientation="vertical"

            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="@dimen/activity_liste_imageview5_height"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/bg" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rcvOyuncular"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_constraintTop_toBottomOf="@+id/imgKapak"
                tools:listitem="@layout/list_item_oyuncu" />

        </LinearLayout>




</androidx.constraintlayout.widget.ConstraintLayout>